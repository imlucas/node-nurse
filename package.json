{
  "author": "Lucas Hrabovsky <hi@imlucas.com>",
  "name": "nurse",
  "description": "Standardized health check",
  "version": "0.1.0",
  "homepage": "http://github.com/imlucas/node-nurse",
  "repository": {
    "type": "git",
    "url": "git://github.com/imlucas/node-nurse.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha"
  },
  "dependencies": {
    "memwatch": "~0.2.2"
  },
  "devDependencies": {
    "mocha": "~1.13.0"
  },
  "engines": {
    "node": "*"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/imlucas/node-nurse/blob/master/LICENSE"
    }
  ],
  "jshintConfig": {
    "node": true,
    "es5": true,
    "boss": true,
    "curly": false,
    "eqeqeq": true,
    "noarg": true,
    "newcap": true,
    "noempty": true,
    "onevar": true,
    "plusplus": false,
    "trailing": true,
    "indent": 4,
    "predef": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
}
